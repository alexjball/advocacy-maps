{
  "name": "@maple-testimony/frontend",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "functions",
    "analysis/downloader"
  ],
  "scripts": {
    "analyze-bundle": "BUNDLE_ANALYZER=true next build && echo open .next/analyze/client.html",
    "build": "next build",
    "build-storybook": "build-storybook -s public",
    "build:functions": "next lint -d functions/src && yarn --cwd functions build",
    "check-formatting": "prettier --check .",
    "check-types": "tsc --noEmit",
    "compose": "docker compose -f infra/docker-compose.yml",
    "deploy:backend:dev": "firebase --project=default deploy --only firestore,functions,storage",
    "deploy:backend:prod": "firebase --project=prod deploy --only firestore,functions,storage",
    "dev": "next dev",
    "dev:backend": "yarn compose up",
    "dev:backend:update": "yarn compose down -v && yarn compose up --build",
    "dev:backfill": "sh ./scripts/backfill-dev.sh",
    "export": "next build && next export",
    "export-test-data": "firebase --project demo-dtp emulators:export --only firestore,storage tests/integration/exportedTestData",
    "export:nolint": "next build --no-lint && next export",
    "firebase-admin": "ts-node -P tsconfig.script.json scripts/firebase-admin",
    "fix": "next lint --fix && prettier --write .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "seed": "jest -c tests/jest.seed.config.ts",
    "serve": "firebase emulators:start --only hosting",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "test:integration": "jest -c tests/jest.integration.config.ts -w 1",
    "test:integration-ci": "node scripts/test-integration-ci.js",
    "test:system": "jest -c tests/jest.system.config.ts",
    "typesense-admin": "ts-node -P tsconfig.script.json scripts/typesense-admin.ts"
  },
  "dependencies": {
    "@alexjball/react-instantsearch-hooks-web": "~6.29.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@react-aria/ssr": "^3.2.0",
    "@react-aria/utils": "^3.13.1",
    "@reduxjs/toolkit": "^1.8.3",
    "autolinker": "^3.16.0",
    "awesome-debounce-promise": "^2.1.0",
    "bootstrap": "5.1.3",
    "clsx": "^1.2.1",
    "copy-to-clipboard": "^3.3.1",
    "dompurify": "^2.3.10",
    "firebase": "^9.6.10",
    "fuse.js": "6.5.3",
    "instantsearch.css": "^7.4.5",
    "instantsearch.js": "^4.43.0",
    "lodash": "^4.17.21",
    "logrocket": "^3.0.1",
    "luxon": "^2.3.1",
    "nanoid": "^3.3.1",
    "next": "^12.3.1",
    "react": "^18.2.0",
    "react-async-hook": "^4.0.0",
    "react-bootstrap": "^2.1.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "react-redux": "^8.0.2",
    "react-select": "^5.2.2",
    "runtypes": "6.5.1",
    "styled-components": "^5.3.3",
    "typesense": "^1.2.2",
    "typesense-instantsearch-adapter": "^2.4.0",
    "usehooks-ts": "^2.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@next/bundle-analyzer": "^12.3.1",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^12.1.10",
    "@tsconfig/node16": "^1.0.2",
    "@types/dompurify": "^2.3.3",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/luxon": "^2.0.9",
    "@types/node": "^17.0.36",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/styled-components": "^5.1.21",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "clsx": "^1.2.1",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.0.9",
    "eslint-config-prettier": "^8.3.0",
    "firebase-admin": "^10",
    "firebase-tools": "^11.7.0",
    "ini": "^1.3.5",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "lorem-ipsum": "^2.0.4",
    "prettier": "2.5.1",
    "sass": "^1.49.0",
    "sb": "^6.5.10",
    "sort-package-json": "^1.57.0",
    "storybook-addon-next": "^1.6.7",
    "ts-node": "^10.5.0",
    "typescript": "4.5.5",
    "yargs": "^17.5.1"
  },
  "packageManager": "yarn@3.2.3"
}
